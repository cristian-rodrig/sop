/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "paths": {
      "@app/*": ["src/app/*"],
      "@env": ["src/environments/environment"],
      "@core/*": ["src/app/core/*"],
      "@shared/*": ["src/app/shared/*"],
      "@ls": ["src/app/core/services/localstorage.service.ts"]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": ["src/main.ts", "src/polyfills.ts", "./src/app/shared/utils/combo-search/ComboSearchComponent.ts"],
  "include": ["src/**/*.d.ts"]
}
